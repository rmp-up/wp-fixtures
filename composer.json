{
  "name": "rmp-up/wp-fixtures",
  "description": "Expressive fixtures persistence in WordPress (and some plugins)",
  "license": "proprietary",
  "autoload": {
    "psr-4": {
      "RmpUp\\WordPress\\Fixtures\\": "lib/"
    },
    "files": [
      "lib/wp-cli.php"
    ]
  },
  "require": {
    "nelmio/alice": "~2|~3",
    "wp-cli/entity-command": "^2.0"
  },
  "require-dev": {
    "johnpbloch/wordpress": "5.2.*",
    "php-coveralls/php-coveralls": "1.1.*",
    "phpstan/phpstan": "0.11.*",
    "phpunit/phpunit": "7.5.*|8.2.*",
    "pretzlaw/phpunit-docgen": "3.0.*",
    "pretzlaw/wp-integration-test": "0.2.*",
    "roave/security-advisories": "dev-master",
    "squizlabs/php_codesniffer": "3.4.*",
    "symfony/yaml": "3.4.*",
    "wp-cli/config-command": "2.0.*",
    "wp-cli/core-command": "2.0.*",
    "wp-cli/db-command": "2.0.*",
    "wp-cli/extension-command": "2.0.*",
    "wp-cli/wp-cli": "2.2.*",
    "wpackagist-plugin/woocommerce":"3.6.*",
    "wpackagist-plugin/yith-woocommerce-product-bundles":"1.1.*"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "installer-paths": {
      "srv/wp-content/plugins/{$name}": [
        "type:wordpress-plugin"
      ],
      "srv/wp-content/themes/{$name}": [
        "type:wordpress-theme"
      ]
    },
    "wordpress-install-dir": "srv"
  },
  "repositories":[
    {
      "type":"composer",
      "url":"https://wpackagist.org"
    }
  ],
  "authors": [
    {
      "name": "rmp up",
      "email": "mail@rmp-up.de"
    }
  ],
  "keywords": [
    "WordPress",
    "Fixture",
    "Faker",
    "Data",
    "Test"
  ]
}
